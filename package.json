    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "dotenv-webpack": "^8.1.0",
    "nodemon": "^3.1.9"
  }
