{
  "name": "shiksha-dost-backend",
  "version": "1.0.0",
  "description": "This is a scalable, secure and modular backend for ShikshaDost",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "dev:prod": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "babel src --out-dir dist --ignore 'node_modules/**' --extensions '.js,.mjs,.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harmeet10000/Shiksha-Dost-Backend.git"
  },
  "keywords": [
    "Docker",
    "rate-limiting",
    "read-replicas"
  ],
  "author": "Harmeet Singh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Harmeet10000/Shiksha-Dost-Backend/issues"
  },
  "homepage": "https://github.com/Harmeet10000/Shiksha-Dost-Backend#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.721.0",
    "@aws-sdk/client-ses": "^3.726.1",
    "@aws-sdk/s3-request-presigner": "^3.717.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
